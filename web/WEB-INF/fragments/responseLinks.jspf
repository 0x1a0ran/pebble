<div class="contentItemLinks">
  <c:if test="${blogEntry.trackBacksEnabled or blogEntry.commentsEnabled}">
    <a href="${blogEntry.permalink}#responses">Responses[<fmt:formatNumber value="${blogEntry.numberOfResponses}"/>]</a>
    <a href="responses/rss.xml?entry=${blogEntry.id}"><img src="common/images/feed-icon-10x10.png" alt="RSS feed for responses to this blog entry" border="0" /></a>
  </c:if>
</div>